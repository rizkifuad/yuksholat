{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "array-type": false,
        "ban-types": false,
        "max-line-length": {
            "options": [
                120
            ]
        },
        "member-access": false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "options": [
                "debug",
                "info",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
